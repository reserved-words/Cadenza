
<TrackLink Typo="@Typo.h6" Id="@Id" Title="@Title" />
<ArtistLink Typo="@Typo.body1" Id="@ArtistId" Name="@ArtistName" />

@code
{
    [Parameter]
    public TrackInfo Model { get; set; }

    public int ArtistId => Model?.ArtistId ?? 0;
    public string ArtistName => Model?.ArtistName ?? "Artist Name";
    public string Id => Model?.Id;
    public string Title => Model?.Title ?? "Track Title";
}