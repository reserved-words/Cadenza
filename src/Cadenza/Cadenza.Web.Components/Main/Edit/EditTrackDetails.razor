@inherits EditTrackDetailsBase

@if (Model != null)
{
    <MudTextField MaxLength="100"
                  Label="Title"
                  @bind-Value="Model.Title"
                  For="@(() => Model.Title)" />
    <MudTextField MaxLength="4"
                  Label="Year"
                  @bind-Value="Model.Year"
                  For="@(() => Model.Year)"
                  Mask="@(new PatternMask("0000"))" />
}
