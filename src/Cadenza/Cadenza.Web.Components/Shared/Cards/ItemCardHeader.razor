
<AppCardHeader Icon="@Type.GetIcon()">

    <MainContent>
        @ChildContent
    </MainContent>
    
</AppCardHeader>

@code {
    [Parameter]
    public PlayerItemType Type { get; set; }

    [Parameter]
    public RenderFragment ChildContent { get; set; }

    [Parameter]
    public LibrarySource? Source { get; set; }

    [Parameter]
    public string Id { get; set; }

}