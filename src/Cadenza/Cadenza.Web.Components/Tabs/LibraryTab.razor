@inherits LibraryTabBase

@if (CurrentItem == null || CurrentItem.Value.Type == null)
{
    <GroupingsTab />
}
else if (CurrentItem.Value.Type == PlayerItemType.Album)
{
    <AlbumTab />
}
else if (CurrentItem.Value.Type == PlayerItemType.Artist)
{
    <ArtistTab />
}
else if (CurrentItem.Value.Type == PlayerItemType.Genre)
{
    <GenreTab />
}
else if (CurrentItem.Value.Type == PlayerItemType.Grouping)
{
    <GroupingTab />
}
else if (CurrentItem.Value.Type == PlayerItemType.Tag)
{
    <TagTab />
}
else if (CurrentItem.Value.Type == PlayerItemType.Track)
{
    <TrackTab />
}