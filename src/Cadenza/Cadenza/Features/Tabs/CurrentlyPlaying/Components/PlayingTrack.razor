@inherits TrackViewBase

<ItemCard Tags="@Model.Tags">
    <ItemCardTitle ItemType="@PlayerItemType.Track" Id="@Model.Id" Name="@Model.Title" />
    <ItemLink Typo="Typo.body1" Type="@PlayerItemType.Artist" Id="@Model.ArtistId" Name="@Model.ArtistName" />
    <MudText Typo="Typo.body1">@Model.Year</MudText>
    <MudText Typo="Typo.body1">@Model.TrackDuration()</MudText>
</ItemCard>