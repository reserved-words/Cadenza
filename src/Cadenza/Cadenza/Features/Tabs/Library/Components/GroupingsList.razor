
<AppCardScrollContainer HeaderPaddingTop="4" HeaderPaddingBottom="0">
    <HeaderContent>
        <ItemCardTitle ItemType="@PlayerItemType.Library" Name="Groupings" Link="false" Typo="@Typo.h5" mb="3" />
    </HeaderContent>
    <ChildContent>
        <AppTable T="@string"
                  Items="@Groupings">
            <RowTemplate>
                <MudTd>
                    <ItemLink Typo="@Typo.body1" Type="@PlayerItemType.Grouping" Id="@context" Name="@context" />
                </MudTd>
            </RowTemplate>
        </AppTable>
    </ChildContent>
</AppCardScrollContainer>

@code {
    [Parameter] public IReadOnlyCollection<string> Groupings { get; set; }
}
