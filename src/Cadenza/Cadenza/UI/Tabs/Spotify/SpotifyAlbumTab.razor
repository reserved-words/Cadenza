@inherits SpotifyAlbumTabBase

<TabGrid>
    
    <TabGridColumn md="3">
        <TabGridRow RowHeight="@RowHeight.Stretch">
            <SpotifyAlbumDetail Model="@Model.Album" />
        </TabGridRow>
    </TabGridColumn>
    
    <TabGridColumn md="9">
        <TabGridRow RowHeight="@RowHeight.Stretch">
            <SpotifyAlbumTracks Tracks="@Model.Tracks" />
        </TabGridRow>
    </TabGridColumn>

</TabGrid>