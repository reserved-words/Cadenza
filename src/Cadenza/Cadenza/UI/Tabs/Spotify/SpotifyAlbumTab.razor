@inherits SpotifyAlbumTabBase

<SubGrid>
    
    <SubGridColumn md="3">
        <SubGridRow RowHeight="@RowHeight.Stretch">
            <SpotifyAlbumDetail Model="@Model.Album" />
        </SubGridRow>
    </SubGridColumn>
    
    <SubGridColumn md="9">
        <SubGridRow RowHeight="@RowHeight.Stretch">
            <SpotifyAlbumTracks Tracks="@Model.Tracks" />
        </SubGridRow>
    </SubGridColumn>

</SubGrid>