@inherits SpotifyPlaylistTabBase

<SubGrid>
    <SubGridColumn md="3">
        <SubGridRow RowHeight="@RowHeight.Stretch">
            <SpotifyPlaylistDetail Model="@Model.Playlist" />
        </SubGridRow>
    </SubGridColumn>
    
    <SubGridColumn md="9">
        <SubGridRow RowHeight="@RowHeight.Stretch">
            <SpotifyPlaylistTracks Tracks="@Model.Tracks" />
        </SubGridRow>
    </SubGridColumn>
</SubGrid>