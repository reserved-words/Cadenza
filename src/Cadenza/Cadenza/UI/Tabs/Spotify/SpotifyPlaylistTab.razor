@inherits SpotifyPlaylistTabBase

<TabGrid>
    <TabGridColumn md="3">
        <TabGridRow RowHeight="@RowHeight.Stretch">
            <SpotifyPlaylistDetail Model="@Model.Playlist" />
        </TabGridRow>
    </TabGridColumn>
    
    <TabGridColumn md="9">
        <TabGridRow RowHeight="@RowHeight.Stretch">
            <SpotifyPlaylistTracks Tracks="@Model.Tracks" />
        </TabGridRow>
    </TabGridColumn>
</TabGrid>