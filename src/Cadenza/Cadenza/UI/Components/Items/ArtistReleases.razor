@using Cadenza.Core.Model

@if (Releases != null)
{
    <AppCardScrollContainer>
        <ChildContent>
            <MudTabs>
                @foreach (var group in Releases)
                {
                    <ArtistReleaseGroupView Model="@group" />
                }
            </MudTabs>
        </ChildContent>
    </AppCardScrollContainer>
}

@code {
    [Parameter]
    public List<ArtistReleaseGroup> Releases { get; set; }
}
