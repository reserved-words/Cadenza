<AppCardDivided>
    <HeaderContent>
        <TrackCardHeader Model="@Model"
                         TitleOnly="false"
                         TabHeading="@TabHeading" />
    </HeaderContent>
    <ChildContent>
        <TagsView Model="@Model.Tags" />
    </ChildContent>
</AppCardDivided>

@code {
    [Parameter]
    public TrackInfo Model { get; set; } = new();

    [Parameter]
    public bool TabHeading { get; set; }
}
