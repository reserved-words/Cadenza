@if (Artist != null)
{
    <AppCardDivided>
        <HeaderContent>
            <ArtistCardHeader Model="@Artist"
                              TitleOnly="false"
                              TabHeading="true" />
        </HeaderContent>
        <ChildContent>
            <TagsView Model="@null" />
        </ChildContent>
    </AppCardDivided>
}

@code {
    [Parameter]
    public ArtistInfo Artist { get; set; } = new();
}
