

@foreach (var link in Model.Links())
{
    <MudPaper Class="pa-0 mb-2"
              Elevation="0">
        <MudLink Href="@link.Url" Target="_blank">
            <MudButton StartIcon="@link.GetIcon()">@link.Type</MudButton>
        </MudLink>

    </MudPaper>
}

@code {
    [Inject]
    public NavigationManager NavigationManager { get; set; }

    [Parameter]
    public ArtistInfo Model { get; set; }
}
