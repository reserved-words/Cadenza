@inherits ToolbarBase

<MudPaper Elevation="3" Outlined="true" Class="d-flex flex-row ma-0 pa-0">
    <MudIconButton Icon="@Icons.Material.Filled.Menu" Style="flex: unset;"/>
    <MudIconButton Icon="@Icons.Material.Filled.Refresh" OnClick="@OnClearSession" Style="flex: unset;" Class="mr-auto" />

    @foreach (var sourceStatus in ConnectorStatuses)
    {
        <ToolbarSource Model="@sourceStatus" />
    }

</MudPaper>