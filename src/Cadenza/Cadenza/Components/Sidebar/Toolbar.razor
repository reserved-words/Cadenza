@inherits ToolbarBase

<MudToolBar DisableGutters="true" Dense="true">
    <MudIconButton Icon="@Icons.Material.Filled.Menu" Edge="Edge.Start" />
    <MudIconButton Icon="@Icons.Material.Filled.Refresh" OnClick="@OnClearSession" Edge="Edge.Start" Class="mr-auto" />

    @foreach (var sourceStatus in ConnectorStatuses)
    {
        <ToolbarSource Model="@sourceStatus" />
    }

</MudToolBar>