@inherits ToolbarBase

<MudPaper Elevation="3" Outlined="true" Class="d-flex flex-row">
    <MudIconButton Icon="@Icons.Material.Filled.Sync" OnClick="@OnSync" Style="flex: unset;" Class="mr-auto" Title="Resync libraries" />
    <MudIconButton Icon="@Icons.Material.Filled.Refresh" OnClick="@OnClearSession" Style="flex: unset;" Class="mr-auto" Title="Refresh connections" />

    @foreach (var sourceStatus in ConnectorStatuses)
    {
        <ToolbarSource Model="@sourceStatus" />
    }

</MudPaper>