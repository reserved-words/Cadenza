
@if (Model != null)
{
    <MudStack Spacing="0">
        <ItemLink Typo="@Typo.body1" Type="@PlayerItemType.Album" Id="@Model.Id" Name="@Model.Title"></ItemLink>
        <MudText Typo="Typo.body2">@Model.ReleaseType.GetDisplayName()</MudText>
        <MudText Typo="Typo.body2">@Model.Year</MudText>
    </MudStack>
}

@code {
    [Parameter] public AlbumVM Model { get; set; }
}